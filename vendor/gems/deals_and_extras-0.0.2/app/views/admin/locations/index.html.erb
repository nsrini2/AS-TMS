<%= will_paginate @offers %>
<%= page_entries_info @offers %>
<table>
    <thead>
        <tr>
            <th>id</th>
            <th>description</th>
            <th>latitude</th>
            <th>longitude</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        <% @locations.each do |l| -%>
            <tr>
                <td><%= l.id %></td>
                <td><%= l.description %></td>
                <td><%= l.latitude %></td>
                <td><%= l.longitude %></td>
                <td>
                    <%= link_to "U", update_latlon_admin_location_path(l) %> |
                    <%= link_to "E", edit_admin_location_path(l) %> |
                    <%= link_to "S", admin_location_path(l) %>
                </td>
            </tr>
        <% end -%>
    </tbody>
</table>
<%= will_paginate @offers %>
<%= page_entries_info @offers %>