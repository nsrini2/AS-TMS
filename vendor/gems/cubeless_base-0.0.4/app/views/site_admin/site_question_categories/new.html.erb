<h2>New Question Category</h2>

<% form_for(:site_question_category, @site_question_category, 
						:url => site_admin_site_question_categories_path,
						:html => {:class => "admin_form"}, 
						:builder => AdminFormBuilder) do |f| %>

	<%= render :partial => "form", :locals => {:f => f} %>
  
	<div class="buttons">
    <%= f.submit "Create", :class => "button" %>
		<input type="submit" name="reset" value="Cancel" class="button light">
  </div>
<% end %>


