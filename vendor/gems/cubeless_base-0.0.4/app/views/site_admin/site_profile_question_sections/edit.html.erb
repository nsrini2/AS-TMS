<h2>Editing Profile Question Section</h2>

<% form_for(:site_profile_question_section, @site_profile_question_section,
 						:url => site_admin_site_profile_question_section_path(@site_profile_question_section),
						:builder => AdminFormBuilder, 
						:html => {:class => "admin_form", :method => :put}) do |f| %>
	<%= render :partial => "form", :locals => {:f => f} %>

	<div class="buttons">
    <%= f.submit "Update", :class => "button" %>
		<input type="submit" name="reset" value="Cancel" class="button light">
  </div>
<% end %>


