<table class="explore_table">
	<thead>
		<tr>
			<!-- <th colspan="2">Person</th> -->
			<th>Departure Location</th>
			<th>Arrival Locations</th>
			<th>Start Travel Time</th>
			<th>End Travel Time</th>
			<th colspan="2"></th>
			<th>Share</th>
		</tr>
	</thead>
	<tbody>
		<% @travels.each do |travel| -%>  
			<%= render :partial => 'travels/my_travel', :locals => { :travel => travel, :classname => cycle('odd', 'even') } %>
		<% end -%>		
	</tbody>
</table>