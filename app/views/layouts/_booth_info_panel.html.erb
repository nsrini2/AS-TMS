<% content_for :booth_info_panel do %>
   <div id="booth_info">
        <div id="follow">
          <%= render "shared/booth_member_table" %>
          <%= action_list_for([(link_for_booth_action(@group) unless hide_for_sponsor)], :class => 'group_actions full_screen') -%>
        </div>

        <div id="booth_tags">
            <%# tag_cloud @group_blog_tags, %w(vb b m s) do |tag, css_class| %>
               <%#= link_to tag.name, { :action => :tag, :id => tag.name }, :class => css_class %>
            <%# end %>
        </div>
  
        <div id="booth_useful_links">
            <h5>Interesting links</h5>
             <% @group_links.each do |link| -%>
              <span class="click_me">
                 <%= link_to link.text, link.url, :target => "_blank"-%>
              </span>
              <br/>
           <%end%>
        </div>
 
   </div>
<% end %>

<%= render :file => 'layouts/_booth_social_feeds_panel' %>
